reference:
  express from require 'express'

object HelloHTTP 1.0.0

Instantiate task
  set @app from ~express
  
  @app.get '/', task as req, res
    res.send 'Hello HTTP!'

  set @server from @app.listen 3000, 'localhost', task
    with @server.address()
      debug 'Example app listening at http://${.address}:${.port}'       
      
   