#!/usr/bin/env sai-run

reference:
  express from require 'express'

object HelloHTTP main 1.0.0

Instantiate task
  set my app from express
  
  set mycroft 1
  
  debug mycroft
  debug @croft
  
  my app.get '/', task given req, res
    res.send 'Hello HTTP!'

  set my server from @app.listen 3000, 'localhost', task
    with my server.address()
      debug 'Example app listening at http://${.address}:${.port}'       
      
   